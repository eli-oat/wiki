{
  "name": "wiki",
  "version": "1.0.0",
  "description": "A static wiki",
  "main": "index.html",
  "author": "eli_oat",
  "license": "cc0-1.0",
  "scripts": {
    "clean": "rm -rf ./dist && rm -rf ./.cache",
    "dev": "export NODE_ENV=dev && npx parcel index.html",
    "build": "yarn run clean && export NODE_ENV=prod && npx parcel build index.html -d ./dist"
  },
  "dependencies": {
    "jquery": "^3.4.1",
    "marked": "^0.7.0",
    "navigo": "^7.1.2",
    "wiki": "^0.19.0"
  },
  "devDependencies": {
    "parcel-bundler": "^1.12.3"
  }
}
